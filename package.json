{
  "name": "psychological-testing-app",
  "version": "0.0.0",
  "main": "mainElectron.js",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "server": "node backend/server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron .",
    "package:win": "ng build && electron-packager . psychological-testing-app --platform=win32 --arch=x64 --out=release-builds --overwrite",
    "package:linux": "ng build && electron-packager . psychological-testing-app --platform=linux --arch=x64 --out=release-builds --overwrite",
    "package:all": "npm run package:win && npm run package:linux"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.14",
    "@angular/material-moment-adapter": "^20.2.9",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "archiver": "^7.0.1",
    "base64-arraybuffer": "^1.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "moment": "^2.30.1",
    "mongoose": "^8.16.4",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.0",
    "@electron/packager": "^18.3.6",
    "@types/jasmine": "~5.1.0",
    "@types/moment": "^2.11.29",
    "electron": "^37.2.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  }
}
